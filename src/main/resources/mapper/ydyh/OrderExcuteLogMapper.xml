<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pda.api.mapper.slave.OrderExcuteLogMapper">

    <select id="selectCheckedExcuteLog" resultType="com.pda.api.domain.entity.OrderExcuteLog">
        select *
        from order_excute_log
        where patient_id = #{patientId} and type = #{type}
        and check_status = '1'
        <if test='orderNos != null and orderNos.size() > 0'>
            and order_no in
            <foreach collection="orderNos" index="index" item="orderNo" separator="," close=")" open="(">
                #{orderNo}
            </foreach>
        </if>
        order by check_time
    </select>
</mapper>
